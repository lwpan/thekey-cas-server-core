<?xml version="1.0" encoding="UTF-8"?>
<flow xmlns="http://www.springframework.org/schema/webflow"
   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
   xsi:schemaLocation="http://www.springframework.org/schema/webflow
             http://www.springframework.org/schema/webflow/spring-webflow-2.0.xsd">

	<on-start>
		<evaluate expression="ticketDestroyerBean.destroyCASTicket(externalContext,messageContext)"/>
	</on-start>
	   	   	
	<view-state id="logout" view="logout">
		<transition on="signin" to="login"/>
	</view-state>
	   	   	
   	<end-state id="login" view="externalRedirect:contextRelative:/login.htm"/>

</flow>